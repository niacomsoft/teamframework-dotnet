    <ProjectReference Include="..\Tfx.Extensions.DependencyInjection.UnityContainer\Tfx.Extensions.DependencyInjection.UnityContainer.csproj" />
    <ProjectReference Include="..\Tfx.Extensions.Logging.Abstractions\Tfx.Extensions.Logging.Abstractions.csproj" />
    <ProjectReference Include="..\Tfx.Extensions.Logging.NLog\Tfx.Extensions.Logging.NLog.csproj" />
    <ProjectReference Include="..\Tfx.Extensions.Logging\Tfx.Extensions.Logging.csproj" />
  </ItemGroup>

